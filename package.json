{
  "name": "lifeposmo-backend",
  "version": "1.0.0",
  "description": "Life postmortem app back-end",
  "main": "index.js",
  "scripts": {
    "start": "node ./src/bin/app",
    "start:dev": "node ./src/bin/app",
    "test": "NODE_ENV=test node_modules/.bin/jest --config=./tests/configs/jest.config.js --passWithNoTests --runInBand --coverage --detectOpenHandles",
    "test:unit": "NODE_ENV=test node_modules/.bin/jest --config=./tests/configs/jest.config.unit.js --passWithNoTests --runInBand --coverage --detectOpenHandles",
    "test:integration": "NODE_ENV=test node_modules/.bin/jest --config=./tests/configs/jest.config.integration.js --passWithNoTests --runInBand --coverage --detectOpenHandles",
    "test:update": "NODE_ENV=test node_modules/.bin/jest -u --runInBand",
    "lint": "./node_modules/.bin/eslint src/",
    "lint:fix": "./node_modules/.bin/eslint src/ --fix",
    "postinstall": "link-module-alias"
  },
  "_moduleAliases": {
    "@Bin": "src/bin",
    "@GraphqlService": "src/lib/graphqlServices",
    "@DbService": "src/lib/dbServices",
    "@UserModule": "src/domain/users",
    "@UserService": "src/domain/users/services",
    "@UserRepository": "src/domain/users/repository",
    "@UserApp": "src/domain/users/app",
    "@UserMocks": "src/domain/users/__mocks__",
    "@PostModule": "src/domain/posts/",
    "@PostService": "src/domain/posts/services",
    "@PostRepository": "src/domain/posts/repository",
    "@PostApp": "src/domain/posts/app",
    "@PostMocks": "src/domain/posts/__mocks__"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/momae93/lifeposmo-backend.git"
  },
  "author": "Mickael AU",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/momae93/lifeposmo-backend/issues"
  },
  "homepage": "https://github.com/momae93/lifeposmo-backend#readme",
  "dependencies": {
    "@types/jest": "^24.0.18",
    "apollo-server": "^2.8.1",
    "axios": "^0.19.0",
    "graphql": "^14.4.2",
    "graphql-iso-date": "^3.6.1"
  },
  "devDependencies": {
    "eslint": "^6.2.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "jest": "^24.9.0",
    "link-module-alias": "^1.2.0",
    "nodemon": "^1.19.1"
  }
}
