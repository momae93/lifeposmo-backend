  "dependencies": {
    "@types/jest": "^24.0.18",
    "apollo-server": "^2.8.1",
    "axios": "^0.19.0",
    "graphql": "^14.4.2",
    "graphql-iso-date": "^3.6.1"
  },
  "devDependencies": {
    "eslint": "^6.2.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
