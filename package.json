{
  "name": "lifeposmo-backend",
  "version": "1.0.0",
  "description": "Life postmortem app back-end",
  "main": "index.js",
  "scripts": {
    "start": "nodemon ./src/bin/app",
    "test": "NODE_ENV=test node_modules/.bin/jest --passWithNoTests --runInBand --coverage --detectOpenHandles",
